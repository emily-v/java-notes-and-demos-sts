package scope;

public class Warehouse {

	public static void main(String[] args) {
		
		Box a = new Box();
		a.value = 1000;
		
		Box b = new Box();
		b.value = 2000;
		
		a.printValue();
		b.printValue();
		
		// always shared because it is the static value
		a.static_value = 20;
		a.printStaticValue();
		b.printStaticValue();
	}
}
